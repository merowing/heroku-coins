					<div id="login">
					<% if(typeof(session) === undefined || !session) { %>
						<a href="/login"><span>Вход</span></a>
					<% }else { %>
						<a href="/login"><span class="logout">Панель управления</span></a>
					<% } %>
	
					</div>
				</div>
				
				<div id="pages">
					<ul>
					<% if(typeof(pages) !== "undefined") { %>
						<% if(pages > 1) { %>
						<% for(var i = 0; i < pages; i+=1) { %>
							<li><%=(i+1)%><a href="/category/all/<%=(i+1)%>"></a></li>
						<% }} %>
						
				<% } %>
					</ul>
				</div>
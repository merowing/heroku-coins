<div class="top-menu">
					<div>
						<% if(phones.length > 2) {%>
							<ul>
								<% for(var i = 0; i < 2; i += 1) { %>
									<li><%=phones[i]%></li>
								<% } %>
							</ul>
							<ul>
								<% for(var i = 2; i < phones.length; i += 1) { %>
									<li><%=phones[i]%></li>
								<% } %>
							</ul>
						<% }else { %>
						<ul>
							<% for(var i = 0; i < phones.length; i += 1) { %>
								<li><%=phones[i]%></li>
							<% } %>
						</ul>
						<% } %>
						<nav>
							<ul>
								<li><a href="/">Главная</a></li>
								<li><a href="/delivery">Доставка</a></li>
								<li><a href="/contacts">Контакты</a></li>
							</ul>
						</nav>
